	"authors": [
		"Xeenaa"
	],
	"contact": {
		"homepage": "https://github.com/xeenaa/villager-manager",
		"sources": "https://github.com/xeenaa/villager-manager"
	},
	"license": "CC0-1.0",
	"icon": "assets/xeenaa_villager_manager/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"com.xeenaa.villagermanager.XeenaaVillagerManager"
		],
		"client": [
			"com.xeenaa.villagermanager.XeenaaVillagerManagerClient"
		]
	},
	"mixins": [
		"xeenaa_villager_manager.mixins.json",
		{
			"config": "xeenaa_villager_manager.client.mixins.json",
			"environment": "client"
		}
	],
	"depends": {
		"fabricloader": ">=0.16.7",
